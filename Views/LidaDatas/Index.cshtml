@model IEnumerable<NTest.Models.LidaData>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ImageURL)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SlopeAngle)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ImageClock)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LidarClock)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PrependDist)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GroudDist)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ImageURL)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SlopeAngle)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ImageClock)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LidarClock)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PrependDist)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GroudDist)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
